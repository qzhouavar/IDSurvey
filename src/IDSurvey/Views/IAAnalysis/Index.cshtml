@{
    Layout = "~/views/shared/_Layout_non_responsive.cshtml";
    ViewData["Title"] = "Immediate Advocacy vs. Medical Record Review Analysis";
}
<!-- Styles -->
<script src="~/lib/amcharts_3.20.17.free/amcharts/amcharts.js"></script>
<script src="https://www.amcharts.com/lib/3/serial.js"></script>
<script src="https://www.amcharts.com/lib/3/plugins/export/export.min.js"></script>
<link rel="stylesheet" href="https://www.amcharts.com/lib/3/plugins/export/export.css" type="text/css" media="all" />
<script src="https://www.amcharts.com/lib/3/themes/light.js"></script>

<div class="row tile_count" style="margin-bottom:0;">
    <div class="col-lg-3 col-md-3 col-sm-6 col-xs-12">
        <h1 id="current-quarter"><strong>Quarter: 9</strong></h1>
    </div>
    <div id="loading">
        <img src="~/images/hourglass.gif" width="50" alt="loading..." />
    </div>
</div>
<div class="row" style="margin-bottom:0;">
    <div class="col-xs-12">
        <h1><strong>@ViewData["Title"]</strong></h1>
    </div>
</div>
<div class="hidden-print row" style="margin:5px; margin-left:0;">
    <div class="col-lg-4 col-md-4 col-sm-4 col-xs-4 left" style="padding-left:0">
        <label for="select-quarter" style="font-size:18px;">Select Quarter: </label>
        <select id="select-quarter" style="min-width:40px;max-width:60px; padding-top: 6px; padding-bottom: 5px; height: 28px; margin-bottom: 6px; display:inline-block">
            <option value="1">6</option>
            <option value="2">7</option>
            <option value="3,4,5">8</option>
            <option value="6,7,8" selected="selected">9</option>
        </select>
        <button class="btn btn-sm btn-default" style="display:inline-block" id="select-quarter-btn">Submit</button>
    </div>
    <div class="col-lg-4 col-md-4 col-sm-4 col-xs-4 left">
        <label for="select-month" style="font-size:18px;">Select Month: </label>
        <select id="select-month" style="min-width:40px;max-width:60px; padding-top: 6px; padding-bottom: 5px; height: 28px; margin-bottom: 6px; display:inline-block">
            <option value="0">..</option>
            <option value="1">6</option>
            <option value="2">7</option>
            <option value="3">8-1</option>
            <option value="4">8-2</option>
            <option value="5">8-3</option>
            <option value="6">9-1</option>
            <option value="7">9-2</option>
            <option value="8">9-3</option>
        </select>
        <button class="btn btn-sm btn-default" style="display:inline-block" id="select-month-btn">Submit</button>
    </div>
</div>


<div class="row">
    <div class="col-md-12 col-sm-12 col-xs-12">
        <div class="dashboard_graph">
            <div class="row x_title">
                <div class="col-md-12">
                    <h3><strong></strong></h3>
                </div>

            </div>
            <div class="col-md-12 col-sm-12 col-xs-12">
                <div id="placeholder33" style="height: 260px; display: none" class="demo-placeholder">
                </div>
                <div> 
                    <table id="complaintResult" class="display left table-bordered" style="width:1000px;border-color:black">
                        <thead>
                            <tr>
                                <th rowspan="3">Service Area</th>
                                <th colspan="6">Overall Process Composite-Communication, Courtesy and Respect, and Access and Responsiveness</th>
                                <th colspan="6">Communication Composite-Questions 7, 8, and 9</th>
                                <th colspan="6">Courtesy and Respect Composite-Questions 6 and 10</th>
                                <th colspan="6">Access and Responsiveness Composite-Questions 11 and 12</th>
                            </tr>
                            <tr>
                                <th colspan="2">Total</th>
                                <th colspan="2">Immediate Advocacy</th>
                                <th colspan="2">Medical Record Review</th>
                                <th colspan="2">Total</th>
                                <th colspan="2">Immediate Advocacy</th>
                                <th colspan="2">Medical Record Review</th>
                                <th colspan="2">Total</th>
                                <th colspan="2">Immediate Advocacy</th>
                                <th colspan="2">Medical Record Review</th>
                                <th colspan="2">Total</th>
                                <th colspan="2">Immediate Advocacy</th>
                                <th colspan="2">Medical Record Review</th>
                            </tr>
                            <tr>
                                <th>%</th>
                                <th>N</th>
                                <th>%</th>
                                <th>N</th>
                                <th>%</th>
                                <th>N</th>
                                <th>%</th>
                                <th>N</th>
                                <th>%</th>
                                <th>N</th>
                                <th>%</th>
                                <th>N</th>
                                <th>%</th>
                                <th>N</th>
                                <th>%</th>
                                <th>N</th>
                                <th>%</th>
                                <th>N</th>
                                <th>%</th>
                                <th>N</th>
                                <th>%</th>
                                <th>N</th>
                                <th>%</th>
                                <th>N</th>
                            </tr>
                        </thead>
                    </table>
                </div>
            </div>
            <div class="clearfix"></div>
        </div>
    </div>
</div>
<br />


<div class="row">
    <div class="col-md-12 col-sm-12 col-xs-12">
        <div class="dashboard_graph">
            <div class="row x_title">
                <div class="col-md-12">
                    <h3><strong></strong></h3>
                </div>

            </div>
            <div class="col-md-12 col-sm-12 col-xs-12">
                <div id="placeholder33" style="height: 260px; display: none" class="demo-placeholder">
                </div>
                <div style="width: 45%;">
                    <table id="complaintResultQ19" class="display left table-bordered" style="border-color:black">
                        <thead>
                            <tr>
                                <th rowspan="3">Service Area</th>
                                <th colspan="6">Q19: Using any number from 0 to 10 where 0 is the worst quality of care complaint process possible and 10 is the best quality of care complaint process possible, what number would you use to rate the overall quality of care complaint process?</th>
                             
                            </tr>
                            <tr>
                                <th colspan="2">Total</th>
                                <th colspan="2">Immediate Advocacy</th>
                                <th colspan="2">Medical Record Review</th>
                            </tr>
                            <tr>
                                <th>%</th>
                                <th>N</th>
                                <th>%</th>
                                <th>N</th>
                                <th>%</th>
                                <th>N</th>
                            </tr>
                        </thead>
                    </table>
                </div>
            </div>
            <div class="clearfix"></div>
        </div>
    </div>
</div>
<br />


<script type="text/javascript" src="~/js/MyAppIAAnalysis.js"></script>
<script type="text/javascript" src="~/js/compositeScoreIAAnalysis.js"></script>
<script type="text/javascript" src="~/js/PrintFigure.js"></script>