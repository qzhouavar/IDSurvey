@model IEnumerable<IDSurvey.Models.ContactRate>

@{
    ViewData["Title"] = "Contact Rate";
}


<div class="row">
    <div class="col-md-12 col-sm-12 col-xs-12">
        <div class="dashboard_graph">
            <div class="row x_title">
                <div class="col-md-12">
                    <h3><strong>Contact Rate</strong></h3>
                </div>
            </div>
            <div class="col-md-12 col-sm-12 col-xs-12">
                <div id="placeholder33" style="height: 260px; display: none" class="demo-placeholder"></div>
                <div style="width: 40%;">

                    <table id="contactRate" class="table table-bordered" cellspacing="0" style="font-size:15px; border:solid black">
                        <thead>
                            <tr>
                                <th style="border:1px solid black;background-color:maroon;color:white">
                                   Month
                                </th>

                                <th style="border:1px solid black;background-color:maroon;color:white">
                                    Contacts
                                </th>
                                <th style="border:1px solid black;background-color:maroon;color:white">
                                    Reached
                                </th>
                                <th style="border:1px solid black;background-color:maroon;color:white">
                                    Wrong Numbers
                                </th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var item in Model)
                            {
                                var reached = @item.COMPLETED * 100.0 / @item.TOTAL;
                                var wrongNos = @item.WRONG_NUMBER * 100.0 / @item.TOTAL;
                                <tr>

                                    <td >
                                        @item.Description.Substring(43).ToUpper()
                                    </td>
                               
                                    <td align="right" >
                                        @Html.DisplayFor(modelItem => item.TOTAL)
                                    </td>
                                    <td align="right">
                                        @String.Format(System.Globalization.CultureInfo.CurrentCulture,"{0:0.00}", @reached)%
                                    </td>
                                    <td align="right">
                                        @String.Format(System.Globalization.CultureInfo.CurrentCulture, "{0:0.00}", @wrongNos)%
                                    </td>
                                </tr>
                            }
                        </tbody>
                    </table>
                </div>
            </div>
            <div class="clearfix"></div>
        </div>
    </div>
</div>
<br />
<script src="~/js/contactRate.js"></script>